import random
print("""
1: Rock
2: Paper
3: Scissors""")
user_choice=int(input("Enter your choice:"))
computer_choice= random.randint(1,3)
print("Computer choice is :", computer_choice)
if user_choice == computer_choice:
    print("Draw")
elif(user_choice==1 & computer_choice==3) | (user_choice==2 & computer_choice==1) | (user_choice==3 & computer_choice==2):
    print("You win")
else:
    print("Computer wins")
